    <patterns>
        <pattern>
            <rows>NONE</rows>
            <cols>ORDER_PRESERVING</cols>
            <ctxs>NONE</ctxs>
        </pattern>
         <pattern>
            <rows>NONE</rows>
            <cols>NONE</cols>
            <ctxs>ORDER_PRESERVING</ctxs>
        </pattern>
        <pattern>
            <rows>CONSTANT</rows>
            <cols>CONSTANT</cols>
            <ctxs>CONSTANT</ctxs>
        </pattern>
         <pattern>
            <rows>CONSTANT</rows>
            <cols>NONE</cols>
            <ctxs>CONSTANT</ctxs>
        </pattern>
        <pattern>
            <rows>ADDITIVE</rows>
            <cols>ADDITIVE</cols>
            <ctxs>ADDITIVE</ctxs>
        </pattern>
        <pattern>
            <rows>ADDITIVE</rows>
            <cols>CONSTANT</cols>
