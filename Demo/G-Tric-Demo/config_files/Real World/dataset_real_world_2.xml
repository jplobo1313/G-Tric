        <col_distribution>UNIFORM</col_distribution>
        <col_param_1>10</col_param_1>
        <col_param_2>20</col_param_2>
        <ctx_distribution>UNIFORM</ctx_distribution>
        <ctx_param_1>5</ctx_param_1>
        <ctx_param_2>15</ctx_param_2>
        <contiguity>NONE</contiguity>
