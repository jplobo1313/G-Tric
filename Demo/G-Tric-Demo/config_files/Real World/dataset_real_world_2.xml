    <quality>
        <background_missings>0.2</background_missings>
        <tricluster_missings>0.1</tricluster_missings>
        <background_noise>0.1</background_noise>
        <tricluster_noise>0.15</tricluster_noise>
        <noise_deviation>2</noise_deviation>
        <background_errors>0.1</background_errors>
        <tricluster_errors>0.05</tricluster_errors>
